@using eKorpa.ViewModels
@model ProfilIndexVM

<a id="nazad" class="btn btn-danger" href="/Artikal/"><i class="fa fa-arrow-left" aria-hidden="true"></i></a>
<br />
<div>
    <label> Ime </label>
    <span>@Model.Ime</span> <br />
    <label> Prezime </label>
    <span>@Model.Prezime</span> <br />
    <label> Email </label>
    <span>@Model.Email</span> <br />
    <div id="ajaxDiv" style="border:1px solid black">

    </div>
</div>
<br />

<link href="/css/ProfilIndex.css" rel="stylesheet" />

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js"></script>
<script>
    dodajSlike();
    function dodajSlike() {
        var url = "/Artikal/IndexW?ProfilID=@Model.ID";
        $.get(url, function (odg) {
            $(ajaxDiv).html(odg);
        });
    }
</script>

